{
  "presets": [
    ["@babel/preset-env", {
      "targets": { 
        "node": "10"
      }
    }],
    "@babel/typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": [
        ["module-resolver", {
          "extensions": [".js", ".ts", ".jsx", ".tsx"],
          "alias": {
            "library": "./src/library"
          }
        }]
      ]
    },
    "production": {
      "plugins": [
        ["module-resolver", {
          "extensions": [".js", ".ts", ".jsx", ".tsx"],
          "alias": {
            "library": "./src/library"
          }
        }]
      ]
    },
    "test": {
      "plugins": [
        ["module-resolver", {
          "extensions": [".js", ".ts", ".jsx", ".tsx"],
          "alias": {
            "library": "./src/library"
          }
        }]
      ]
    }
  }
}
